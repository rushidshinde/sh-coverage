# External Webflow Site Configuration
# Get your API token from: https://webflow.com/dashboard/account/integrations
EXTERNAL_WEBFLOW_SITE_TOKEN=your_api_token_here

# Your external Webflow site ID
EXTERNAL_WEBFLOW_SITE_ID=your_site_id_here

# Collection ID to fetch data from
EXTERNAL_WEBFLOW_COLLECTION_ID=your_collection_id_here

# Optional: Secret for webhook verification
WEBHOOK_SECRET=your_webhook_secret_here

# Optional: Base path for Webflow Cloud deployment (e.g., /my-app)
NEXT_PUBLIC_BASE_PATH=
